#include<stdio.h>
#include <string.h>
#define pf printf
#define sf scanf
 
int main()
{ 
 
	char s[101];
	int h[10];
	int i;
	for(i=0;i<10;i++)
		h[i]=0;
	sf("%s",s);
	int n = strlen(s);
	for(i=0;i<n;i++)
	{	//h1[i]=(int)(s[i]-'0');
		h[(s[i]-'0')]++;
 
	}
	for(i=0;i<10;i++)
		pf("%d %d \n",i,h[i]);
 
 
}
