#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define db double 
#define z long long int 
using namespace std ;
#define siz 100001
#define mod 1000000007



int main()
{ 
  z n,k,i;
  cin>>n;
  z a[n];
  for(i=0;i<n;i++) cin>>a[i];
  cin>>k;
  for(i=0;i<n;i++) { if(a[i]==k) {cout <<i; break;}}
  
   

   
